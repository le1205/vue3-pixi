{
  "name": "@vue3-pixi/composables",
  "type": "module",
  "version": "0.6.3",
  "description": "Vue createRenderer for PixiJS",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/vue3-pixi-renderer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/vue3-pixi-renderer.git"
  },
  "bugs": {
    "url": "https://github.com/hairyf/vue3-pixi-renderer/issues"
  },
  "keywords": [
    "pixi.js",
    "vue"
  ],
  "main": "src/index.ts",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      },
      "./*": "./*"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "typesVersions": {
    "*": { "*": ["./dist/*", "./*"] }
  },
  "files": ["dist", "global.d.ts"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "pixi.js": "^7.2.4",
    "vue-demi": "^0.14.1"
  }
}
